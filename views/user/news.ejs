<%- include('../user/includes/layout.ejs') %>

<%- include('../user/includes/User-header.ejs') %>
<div class="breadcrumb-area pt-255 pb-170" style="background-image: url(assets/img/banner/banner-4.jpg)">
    <div class="container-fluid">
        <div class="breadcrumb-content text-center">
            <h2>News And Updates</h2>
        </div>
    </div>
</div>

<div class="blog-area pb-110 mt-5">
    <div class="container">
        <div class="row">
            <% result.forEach(function(result){%>
                <div class="col-lg-4 col-md-6">
                    <div class="blog-hm-wrapper mb-40">
                        <div class="blog-img">
                            <a href="/newsdetails/<%=result.id%>"><img src="/images/News-Images/<%=result.NewsImg[0]%>" alt="image"></a>
                            <div class="blog-date">
                                <h4>24 October, 2022</h4>
                            </div>
                            
                        </div>
                        <div class="blog-hm-content">
                            <h3><a href="/newsdetails/<%=result.id%>"><%=result.Headline%></a></h3>
                            
                        </div>
                    </div>
                </div>
            <%})%>
        </div>
    </div>
</div>


<%- include('../user/includes/User-footer.ejs') %> 