      <div class="main-panel">
        <div class="content-wrapper">
          <div class="row" style="display:block;">
            <div class="col-lg-6 grid-margin stretch-card " style="max-width:100%;">
              <div class="userdiv table-responsive" style="width:100%;">
                
                <table id="example" class="display nowrap mt-5" style="width:100%" class="table" >
                  <thead>
                    <tr>
                        <th>No</th>
                        <th>OrderId</th>
                        <th>Date</th>
                        <th>Name</th>
                        <th>Total Price</th>
                        <th>Payment Method</th>
                        <th>Status</th>
                    </tr>
                  </thead>
                  
                  
                  <tbody>
                    <% result.forEach(function(result,index){%>
                      <tr>
                        <td><%=index +1%></td>
                        <td><%=result._id%></td>
                        <td><%=result.Date%></td>
                        <td><%=result.Address[0].Fullname%></td>
                        <td><%=result.totalPrice%></td>
                        <td><%=result.paymentMode%></td>
                        <td><%=result.paymentStatus%></td>
                      </tr>
                    <%})%>
                                            
                  </tbody>
                  
              </table>
              <hr>
              </div>
